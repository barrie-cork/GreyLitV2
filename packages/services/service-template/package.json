{
  "name": "@grey-lit/service-template",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "lint": "eslint . --ext .ts",
    "test": "jest --setupFiles ./jest.setup.ts",
    "test:watch": "jest --watch --setupFiles ./jest.setup.ts",
    "test:coverage": "jest --coverage --setupFiles ./jest.setup.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@grey-lit/types": "0.1.0",
    "@grey-lit/utils": "0.1.0",
    "express": "^4.19.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^18.17.1",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "ts-node-dev": "^2.0.0"
  }
}
