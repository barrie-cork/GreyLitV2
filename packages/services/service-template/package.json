{
  "name": "@grey-lit/service-template",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "lint": "eslint . --ext .ts",
    "test": "jest",
    "test:unit": "jest \"src/.*\\.test\\.ts$\"",
    "test:integration": "jest \"__tests__/integration/.*\\.test\\.ts$\"",
    "test:e2e": "jest \"__tests__/e2e/.*\\.test\\.ts$\"",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@grey-lit/types": "0.1.0",
    "@grey-lit/utils": "0.1.0",
    "express": "^4.19.2"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-typescript": "^7.23.3",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^18.17.1",
    "@types/supertest": "6.0.2",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "supertest": "7.0.0",
    "ts-jest": "^29.1.2",
    "ts-node-dev": "^2.0.0"
  },
  "type": "module"
}
